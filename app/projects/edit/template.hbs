<div class="mdl-grid">
  <div class="mdl-cell mdl-cell--12-col">
    <h3>{{model.title}}</h3>
  </div>
</div>
<div class="mdl-grid">
  {{#each model.tiles as |tile|}}
    {{project-edit-items tile=tile}}
  {{/each}}
</div>
<div class="actions">
  {{#link-to 'projects.index'}}
    <mdl-button isFloating=true icon='star'>Star</mdl-button>
  {{/link-to}}
</div>
